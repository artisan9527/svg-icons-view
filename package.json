{
	"name": "svg-icons-view",
	"version": "1.0.0",
	"description": "A CLI tool for previewing all SVG files within a project. This tool works by starting a local HTTP server, allowing you to preview the SVG files in your browser.",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"dev": "tsx src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js"
	},
	"bin": {
		"icons": "./dist/index.js"
	},
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.12.12",
		"tsx": "^4.11.0",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.19.2"
	},
	"keywords": [
		"svg",
		"preview",
		"icon"
	]
}
